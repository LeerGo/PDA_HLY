<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WidgetMineMenu">
        <attr name="wmmTitle"
              format="string|reference" />
        <attr name="wmmDesc"
              format="string|reference" />
        <attr name="wmmIcon"
              format="reference" />
    </declare-styleable>
    
    <declare-styleable name="WidgetTitleBar">
        <attr name="wtbTitle"
              format="string|reference" />
        <attr name="wtbShowBack"
              format="boolean" />
    </declare-styleable>
    
    <declare-styleable name="WidgetPropsItem">
        <attr name="propsKey"
              format="string|reference" />
        <attr name="propsValue"
              format="string|reference" />
        <attr name="propsKeyColor"
              format="color|reference" />
        <attr name="propsValueColor"
              format="color|reference" />
        <attr name="propsKeySize"
              format="dimension" />
        <attr name="propsValueSize"
              format="dimension" />
        <attr name="propsValueGravity">
            <flag name="start"
                  value="0x00800003" />
            <flag name="end"
                  value="0x00800005" />
        </attr>
    </declare-styleable>
    
    <declare-styleable name="WidgetSearchBar">
        <attr name="wsbHint"
              format="string|reference" />
        <attr name="wsbText"
              format="string|reference" />
    </declare-styleable>
</resources>